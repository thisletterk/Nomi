{
  "expo": {
    "name": "Nomi",
    "slug": "nomi",
    "projectId": "nomi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nomi.png",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "description": "Your gentle companion, always here to talk.",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/screen2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://google.com/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/screen1.png",
          "dark": {
            "image": "./assets/images/screen1.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://google.com/"
      },
      "eas": {
        "projectId": "8d4fe029-b1f7-405b-abec-0a4f6cb7c2ea"
      }
    },
    "owner": "kingskiss_"
  }
}
